{
    "name": "mohsin-shaikh/learn-filament-admin",
    "type": "project",
    "description": "Learn Filament Admin.",
    "keywords": ["filament", "admin", "framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bezhansalleh/filament-shield": "^1.1",
        "doctrine/dbal": "^3.3",
        "filament/filament": "^2.9",
        "filament/spatie-laravel-media-library-plugin": "^2.9",
        "filament/spatie-laravel-settings-plugin": "^2.9",
        "filament/spatie-laravel-tags-plugin": "^2.9",
        "filament/spatie-laravel-translatable-plugin": "^2.9",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "shuvroroy/filament-spatie-laravel-backup": "^1.1",
        "shuvroroy/filament-spatie-laravel-health": "^1.3",
        "spatie/laravel-health": "^1.7",
        "spatie/laravel-settings": "^2.3",
        "squirephp/countries-en": "^3.2",
        "squirephp/currencies-en": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
